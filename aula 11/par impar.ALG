Algoritmo "semnome"

Var
num,par,impar: inteiro

Inicio
escreval("Digite um número ")
leia(num)

num <- (num)
se num mod 2 = 0 entao
  par <- (num + 5 )
  escreval ("A soma desses números é:", par)
senao
    impar <- (num + 8 )
     escreval("A soma desse números é:",impar)
fimse

Fimalgoritmo